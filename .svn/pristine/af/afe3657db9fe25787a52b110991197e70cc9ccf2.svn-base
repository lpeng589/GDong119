.add_btn a{display: block; float: left; width: 2.413333rem; height: 2.413333rem; background: #c3c3c3 url(../images/bg/bg11.png) center no-repeat; background-size: 1.16rem; border-radius: 0.066667rem; -webkit-box-flex: 1; -webkit-flex: 1; -ms-flex: 1; flex: 1; margin-right: 0.733333rem;}
.add_btn a:last-child{margin-right: 0;}
.send_btn{display: block; height: 1.306667rem; line-height: 1.306667rem; text-align: center; color: #fff; font-size: 0.4rem; background: #4eb5f1; border-radius: 0.133333rem;}

/*问题反馈*/
.question_list .question_item{height: 2.746667rem; padding: 0.4rem 0.426667rem;}
.question_list .question_item .q_img{width: 1.946667rem; height: 1.946667rem; margin-right: 0.4rem;}
.question_list .question_item p{color: #808080; font-size: 0.32rem; line-height: 0.56rem; overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}
