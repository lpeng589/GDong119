.page{
	max-width: 640px;
	margin:0 auto;
	background-color: #fafafa;
	height: 100%;
}
.estatus{
	padding:10px;
	background: #fff;
}
.estatus1{
	font-weight: bold;
	color: #000;
	font-size: 16px;
}
.estatus2{
	font-weight: bold;
	color: #f11;
	font-size: 16px;
}
.ehead{
	margin-top:2px;
	background: #fff;
	padding:10px;
}
.headimg{
	margin:0 4px;
	width: 10%;
}
.headimg img{
	width: 36px;
}
.headp{
	width: 86%;
}
.headp p{
	line-height: 24px;
	font-size: 14px;
	margin-left:0 4px;
	color: #888;
}
.evideo{
	background: #fff;
}
.evideo video{
	width:60%;
	margin:20px 20%;
}
.etextarea{
	width:90%;
	margin:0px 5%;
	height: 100px;
	border: 1px solid #888;
	color: #333;
	padding:10px;
}
.etime,.eaddress{
	color: #f00;
	margin:0 4px;
}
.eimg{
	background: #fff;
	padding:0 10px;
}
.eimg div{
	border-radius:10px;
}
.eimg img{
	padding:10px;
	width:64px;
	height:64px;
}
.econtent{
	padding:10px;
	font-size: 14px;
	background: #fff;
	margin-bottom: 2px;
}
.esubmit{
	margin:10px;
	text-align: center;
	margin-bottom: 30px;
}
.esubmit span{
	background: #3bafaf;
	padding:10px 100px;
	color: #fff;
}
.evoice{
	background: #000;
}
