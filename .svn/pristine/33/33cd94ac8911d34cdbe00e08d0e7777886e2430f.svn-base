/**app.wxss**/
body {
  display: block;
  min-height: 100%;
  background-color: #EFEFEF;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200px 0;
  box-sizing: border-box;
} 
.bg{
   background-color: #337AB0;
}
/*开关控制样式*/
.checkmain{
    margin-top: 8px;
    position: relative;
}
.checkbg{
    width: 64px;
    height: 32px;
    border-radius: 25px;
    background-color:  #888888;
    opacity: 0.2;
}
.ispublic{
    width: 32px;
    height: 32px;
    border-radius: 50%;
    background-color: #337AB0;
    transition: 0.7s;
    position: absolute;
    right: 0px;
    bottom: 0px;
}
.unpublic{
    width: 32px;
    height: 32px;
    border-radius: 50%;
    background-color: #576b95;
    opacity: 0.1;
    transition: 0.7s;
    position: absolute;
    bottom: 0px;
}
/*其他初始化默认样式*/
.inline{
	display: inline-block;
}
.relative{
    position: relative;
}
.absolute{
    position: absolute;
}
.fl{
    float: left;
}
.fr{
    float: right;
}
.middle{
    vertical-align: middle;
}
.hide{
    display: none;
}
.page-header {
  display: flex;
  font-size: 32px;
  color: #aaa;
  margin-top: 50px;
  flex-direction: column;
  align-items: center;
}
.page-header-text {
  padding: 20px 40px;
}
.page-header-line {
  width: 150px;
  height: 1px;
  border-bottom: 1px solid #ccc; 
}

.page-body {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  flex-grow: 1;
  overflow-x: hidden;
}
.page-body-wrapper {
  margin-top: 100px;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}
.page-body-wrapper form {
  width: 100%;
}
.page-body-wording {
  text-align: center;
  padding: 200px 100px;
}
.page-body-info {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  margin-bottom: 50px;
  width: 100%;
  padding: 50px 0 150px 0;
}
.page-body-title {
  margin-bottom: 100px;
  font-size: 32px;
}
.page-body-text {
  font-size: 30px;
  line-height: 26px;
  color: #ccc;
}
.page-body-text-small {
  font-size: 24px;
  color: #000;
  margin-bottom: 100px;
}
.page-body-form {
  width: 100%;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  width: 100%;
  border: 1px solid #eee;
}
.page-body-form-item {
  display: flex;
  align-items: center;
  margin-left: 30px;
  border-bottom: 1px solid #eee;
  height: 88px;
  font-size: 34px;
}
.page-body-form-key {
  width: 180px;
  color: #000;
}
.page-body-form-value {
  flex-grow: 1;
}
.page-body-form-value .input-placeholder {
  color: #b2b2b2;
}

.page-body-form-picker {
  display: flex;
  justify-content: space-between;
  height: 100px;
  align-items: center;
  font-size: 36px;
  margin-left: 20px;
  padding-right: 20px;
  border-bottom: 1px solid #eee;
}
.page-body-form-picker-value {
  color: #ccc;
}

.page-body-buttons {
  width: 100%;
}
.page-body-button {
  margin: 25px;
}
.page-body-button image {
  width: 150px;
  height: 150px;
}
.page-footer {
  text-align: center;
  color: #1aad19;
  font-size: 24px;
  margin: 20px 0;
}

.green{
    color: #09BB07;
}
.red{
    color: #F76260;
}
.blue{
    color: #10AEFF;
}
.yellow{
    color: #FFBE00;
}
.gray{
    color: #C9C9C9;
}

.strong{
    font-weight: bold;
}

.bc_green{
    background-color: #09BB07;
}
.bc_red{
    background-color: #F76260;
}
.bc_blue{
    background-color: #10AEFF;
}
.bc_yellow{
    background-color: #FFBE00;
}
.bc_gray{
    background-color: #C9C9C9;
}

.tc{
    text-align: center;
}

.page input{
    padding: 20px 30px;
    background-color: #fff;
}
checkbox, radio{
    margin-right: 10px;
}

.btn-area{
    padding: 0 30px;
}
.btn-area button{
    margin-top: 20px;
    margin-bottom: 20px;
}
.page__hd{
    padding: 50px 50px 100px 50px;
    text-align: center;
}
.page__title{
    display: inline-block;
    padding: 20px 40px;
    font-size: 32px;
    color: #AAAAAA;
    border-bottom: 1px solid #CCCCCC;
}
.page__desc{
    display: none;
    margin-top: 20px;
    font-size: 26px;
    color: #BBBBBB;
}

.section{
    margin-bottom: 80px;
}
.section_gap{
    padding: 0 30px;
}
.section__title{
    margin-bottom: 16px;
    padding-left: 30px;
    padding-right: 30px;
}
.section_gap .section__title{
    padding-left: 0;
    padding-right: 0;
}
