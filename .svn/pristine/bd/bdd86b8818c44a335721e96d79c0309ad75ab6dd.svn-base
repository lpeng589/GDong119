.userinfo-avatar {
  width: 80px;
  height: 80px;
  border-radius: 50%;
  vertical-align: middle;
}
.userinfo-nickname {
  color: black;
}
.usermotto {
  margin-top: 200px;
}
.user-motto{
  color: white;
}
.center {
  height: 64px;
  text-align: center;
  color: #000;
}
.show{
  transform:rotate(360deg);
  transition:width 1s,height 1s,transform 1s;
   opacity: 1;
}
.hiddle{
  transition:width 1s,height 1s,transform 1s;
  opacity: 1;
}
.indexbg{
  background-color: #337AB0;
